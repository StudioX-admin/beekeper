{
  "name": "beekeper",
  "version": "1.0.0",
  "description": "스마트폐기물관리 플랫폼",
  "private": true,
  "scripts": {
    "start": "cd server && npm start",
    "install:server": "cd server && npm install",
    "install:web": "cd web && npm install",
    "install:app": "cd app && npm install",
