    "build:app": "cd app && npm run build",
    "dev:server": "cd server && npm run dev",
    "dev:web": "cd web && npm run serve",
    "dev:app": "cd app && npm run serve"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  }
}
